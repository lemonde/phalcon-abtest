{
    "name": "lemonde/phalcon-abtest",
    "description": "An AB testing library for Phalcon applications",
    "type": "library",
    "require-dev": {
        "phalcon/ide-stubs": "^5.0",
        "phpunit/phpunit": "^7"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Le Monde",
            "email": "dt-web@lemonde.fr"
        }
    ],
    "autoload": {
        "psr-4": {
